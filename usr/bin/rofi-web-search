#!/bin/bash

#BROWSER=google-chrome-stable

surfraw -browser=$BROWSER $(sr -elvi | awk -F'-' '{print $1}' | sed '/:/d' | awk '{$1=$1};1' | rofi -location 1 -width 100 -yoffset -30 -lines 9 -columns 2 -font "Noto Sans 13" -kb-row-select "Tab" -kb-row-tab "Control+space" -dmenu -mesg ">>> Tab = Autocomplete" -i -p "Web Search: ")
